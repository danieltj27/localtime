{
    "name": "danieltj/localtime",
    "type": "phpbb-extension",
    "description": "Display the user's local time on their profile.",
    "version": "1.0.0",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Daniel James",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.0"
    },
    "extra": {
        "display-name": "Local Time",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.15"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/local_time/",
            "filename": "version_check",
            "ssl": true
        }
    }
}